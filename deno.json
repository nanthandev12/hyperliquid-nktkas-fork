{
  "name": "@nktkas/hyperliquid",
  "version": "0.25.0",
  "exports": {
    ".": "./src/mod.ts",
    "./signing": "./src/signing/mod.ts",
    "./api/exchange": "./src/api/exchange/~mod.ts",
    "./api/info": "./src/api/info/~mod.ts",
    "./api/subscription": "./src/api/subscription/~mod.ts"
  },
  "imports": {
    "@msgpack/msgpack": "npm:@msgpack/msgpack@^3.1.2",
    "@noble/hashes": "npm:@noble/hashes@^2.0.0",
    "@noble/secp256k1": "npm:@noble/secp256k1@^3.0.0",
    "typescript-event-target": "npm:typescript-event-target@1.1.1",
    "valibot": "npm:valibot@1.1.0"
  },
  "fmt": {
    "lineWidth": 120
  },
  "lint": {
    "rules": {
      "exclude": ["no-slow-types"]
    }
  },
  "publish": {
    "exclude": [
      ".github",
      "build",
      "tests"
    ]
  },
  "license": "MIT",
  "lock": false,
  "tasks": {
    "build:npm": "deno run --allow-env --allow-read --allow-write --allow-run build/npm.ts"
  }
}
